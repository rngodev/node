{
  "name": "rngo",
  "version": "0.1.4",
  "description": "rngo client and CLI",
  "bin": "./bin/run.js",
  "keywords": [
    "api",
    "cli"
  ],
  "author": "rngo",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "yarn compile && NODE_OPTIONS=--experimental-vm-modules jest",
    "dev": "./bin/dev.js",
    "compile": "rm -rf dist/ && tsc && tsc-alias",
    "codegen": "graphql-codegen"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json"
  ],
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@oclif/core": "^2.8.0",
    "@oclif/plugin-help": "^5.2.9",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "decompress": "^4.2.1",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "inquirer": "^9.2.16",
    "json-bigint": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "open": "^10.1.0",
    "ora": "^8.0.1",
    "pg": "^8.11.3",
    "pluralize": "^8.0.0",
    "simple-git": "^3.23.0",
    "ts-results": "^3.3.0",
    "yaml": "^2.4.1",
    "yauzl": "^3.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/client-preset": "^2.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/decompress": "^4.2.4",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.0",
    "@types/json-bigint": "^1.0.4",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "@types/pg": "^8.6.6",
    "@types/pluralize": "^0.0.29",
    "@types/yauzl": "^2",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "np": "^9.2.0",
    "oclif": "^3.7.3",
    "pierre": "^2.0.0-alpha.9",
    "prettier": "2.8.7",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.5",
    "typedoc": "^0.25.7",
    "typescript": "^5.0.3"
  },
  "oclif": {
    "bin": "rngo",
    "dirname": "rngo",
    "commands": "./dist/cli/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "auth": {
        "description": "Authenticate with the rngo API."
      },
      "infer": {
        "description": "Update the simulation config based upon system state."
      },
      "init": {
        "description": "Initialize this repository for use with rngo."
      },
      "sim": {
        "description": "Run the simulation and import the data."
      }
    }
  },
  "packageManager": "yarn@4.0.2"
}
